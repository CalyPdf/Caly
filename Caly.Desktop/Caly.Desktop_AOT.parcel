{
    "GeneralSettings": {
        "NetProjectPath": "Caly.Desktop.csproj",
        "ApplicationName": "Caly Pdf Reader",
        "Version": {
            "$type": "msbuild",
            "property": "Version"
        },
        "PackageName": {
            "$type": "msbuild",
            "property": "AssemblyName"
        },
        "AssemblyName": {
            "$type": "msbuild",
            "property": "AssemblyName"
        }
    },
    "LinuxSettings": {
        "CreateBinSymlink": "True"
    },
    "Win32Settings": {
        "InstallerIcon": "../Logo/Favicon Transparent.ico",
        "InstallerLicense": "../LICENSE",
        "InstallerRequiresAdmin": "False",
        "IncludeUninstaller": "True"
    },
    "MacOsSettings": {
        "CreateBundle": true,
        "BundleIdentifier": "com.CompanyName.Caly",
        "SigningCredentialsType": "AdHoc"
    },
    "PublishSettings": {
        "Configuration": "AOT",
        "ExtraBuildProperties": {}
    }
}